@{
    ViewData["Title"] = "Liste Cartes";
}
@* @model Utilisateur; *@
<h1>@ViewData["Title"]</h1>
<div>
    <a asp-controller="Utilisateur" asp-action="Add">Ajout Carte</a>
</div>
@if(Model.Count == 0)
{
    <div class="alert alert-danger" role="alert">
        Il n'y pas de carte enregistr√©e
    </div>
}

else
{
    @foreach (var item in Model)
    {
        <p>@item.Titre</p>
    }
}