@model List<Projet> 
<h1>Mes projets</h1>
<div>
    <a asp-controller="Projet" asp-action="Add">Ajout Projet</a>
</div>
@if(@Model.Count == 0)
{
    <p>Vous n'avez pas de projet en cours, cr√©ez en un !</p>
}
else
{
    <p>Vous avez @Model.Count projet(s)</p>
}
<ul>
    @foreach(var projet in @Model)
    {
        <li><a href="/Projet/Detail/@projet.Id">@projet.Nom</a></li>

    }
</ul>