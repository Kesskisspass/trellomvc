@{
    ViewData["Title"] = "Liste Projets";
}
@* @model Projet; *@
<h1>@ViewData["Title"]</h1>
<div>
    <a asp-controller="Projet" asp-action="Add">Ajout Projet</a>
</div>
@if(Model.Count == 0)
{
    <div class="alert alert-danger" role="alert">
        Il n'y pas de projet enregistr√©
    </div>
}

else
{
    @foreach (var item in Model)
    {
        <p>@item.Nom</p>
        <ul>
            @foreach(var liste in item.Listes)
            {
                <li>@liste.Nom</li>
            }
        </ul>
        
    }
}